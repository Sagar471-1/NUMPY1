#basics of numpy

import numpy as np 
l =[[1,2,3],[4,5,6],[7,8,9]]
numpy_array = np.array(l)
print(numpy_array)
zeros_array = np.zeros((3,4))
print(zeros_array)

ones_array = np.ones((3,4))      
print(ones_array)  
      #for calling a function write name_array 

random_array = np.random.rand(3,4)
print(random_array)
